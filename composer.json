{
    "name": "mxPHP",
    "description": "An sample PHP framework",
    "keywords": ["PHP", "Framework", "mxPHP", "itaken"],
    "type": "framework",
    "version": "1.0.0",
    "authors": [
        {
            "name": "itaken",
            "email": "regelhh@gmail.com",
            "homepage": "https://github.com/itaken",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "filp/whoops": "^2.3",
        "doctrine/orm": "^2.5.14",
        "catfan/medoo": "~1.6",
        "twig/twig": "~2.6",
        "guzzlehttp/guzzle": "~6.3"
    },
    "require-dev": {
        "symfony/var-dumper": "~3.4",
        "phpunit/phpunit": "~6.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "": "mxjor"
        },
        "classmap": [
            "./mxjor"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "license": "MIT",
    "homepage" : "https://github.com/itaken/mxPHP",
    "support": {
        "email": "regelhh@gmail.com",
        "source": "https://github.com/itaken/mxPHP"
    }
}